{
    "name": "ayra/laravel-themes",
    "description": "Theme will help you organize your themes inside Laravel projects easily and maintain its related assets, layouts and partials for the theme in single directory. (Based on teepluss/theme)",
    "keywords": ["laravel", "theme", "layout", "asset", "widget"],
    "homepage": "https://github.com/imajkumar/laravel-theme-management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ajay Kumar",
            "email": "ajayit2020@gmail.com"
        }
       
    ],
    "require": {
        "php": ">=7.2.0",
        "laravel/serializable-closure": "^1.3"
    },
    "require-dev": {
        "illuminate/support": "^9.19|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ayra\\Theme\\": "src/"
        },
        "classmap": [
            "src/Exceptions.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ayra\\Theme\\ThemeServiceProvider"
            ],
            "aliases": {
                "Theme": "Ayra\\Theme\\Facades\\Theme"
            }
        }
    },
    "minimum-stability": "dev"
}
